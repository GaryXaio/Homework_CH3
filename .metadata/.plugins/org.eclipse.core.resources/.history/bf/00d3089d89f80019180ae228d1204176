package test_p15;

public class test_p15 {

	public static void main(String[] args) {
		Car car1;
		car1 = new Car();
		
		car1.setNumGas(1234,20.5);
		
	}

}

class Car {
	int num;
	double gas;
	
	void setNumGas(int n,double g)
	{
		num = n;
		gas = g;
		System.out.println("將車號設為"+ num +"，汽油量設為"+ gas);
	}
	

}